# WalletConnect Project ID (obtenha em https://cloud.walletconnect.com)
VITE_WALLETCONNECT_PROJECT_ID=your_project_id_here

# URLs de RPC (opcional - para override das configurações padrão)
VITE_MAINNET_RPC_URL=https://mainnet.infura.io/v3/your-infura-key
VITE_SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your-infura-key
VITE_POLYGON_RPC_URL=https://polygon-rpc.com

# Endereços dos contratos (para diferentes redes)
VITE_MAINNET_TOKEN_ADDRESS=0x...
VITE_MAINNET_NFT_ADDRESS=0x...
VITE_SEPOLIA_TOKEN_ADDRESS=0x...
VITE_SEPOLIA_NFT_ADDRESS=0x...

# Configurações da aplicação
VITE_APP_NAME=CryptoNFT DApp
VITE_APP_DESCRIPTION=Compre NFTs usando tokens ERC-20

# Analytics (opcional)
VITE_GA_TRACKING_ID=G-XXXXXXXXXX
